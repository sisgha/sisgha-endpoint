{
  "name": "@sisgha/endpoint",
  "private": true,
  "version": "1.0.0-beta.0",
  "scripts": {
    "plop": "plop",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm",
    "typeorm-db-migration": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm -d src/infrastructure/database/data-sources/migration.data-source.ts",
    "typeorm-db-seed": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm -d src/infrastructure/database/data-sources/seed.data-source.ts",
    "typeorm:entity": "yarn typeorm entity:create",
    "typeorm:generate": "yarn typeorm migration:generate",
    "typeorm:create": "yarn typeorm migration:create",
    "db:migrate": "yarn typeorm-db-migration migration:run",
    "db:rollback": "yarn typeorm-db-migration migration:revert",
    "seed:create": "yarn typeorm-db-seed migration:create",
    "seed:run": "yarn typeorm-db-seed migration:run",
    "seed:rollback": "yarn typeorm-db-seed migration:revert"
  },
  "dependencies": {
    "@apollo/server": "^4.7.3",
    "@casl/ability": "^6.5.0",
    "@keycloak/keycloak-admin-client": "^22.0.1",
    "@nestjs/apollo": "^11.0.6",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/graphql": "^11.0.6",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/throttler": "^3.1.0",
    "@nestjs/typeorm": "^9.0.1",
    "@ucast/core": "^1.10.2",
    "@ucast/mongo": "^2.4.3",
    "@ucast/sql": "^1.0.0-alpha.1",
    "compression": "^1.7.4",
    "eventemitter2": "^6.4.9",
    "graphql": "^16.6.0",
    "graphql-type-json": "^0.3.2",
    "helmet": "^6.0.1",
    "inclusion": "^1.0.1",
    "lodash": "^4.17.21",
    "meilisearch": "^0.32.0",
    "openid-client": "^5.3.1",
    "p-map": "^5.5.0",
    "passport": "^0.6.0",
    "passport-http-bearer": "^1.0.1",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.17",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/compression": "^1.7.2",
    "@types/cron": "^2.0.1",
    "@types/dompurify": "^2.4.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/lodash": "^4.14.191",
    "@types/node": "^16.0.0",
    "@types/passport-http-bearer": "^1.0.37",
    "@types/passport-jwt": "^3.0.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "change-case": "^4.1.2",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "plop": "^3.1.2",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-morph": "^16.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "^5.1.3"
  }
}
